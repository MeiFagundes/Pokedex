    "version": "0.2.0",
    "configurations": [
        {
            "name": "Flutter run",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "-d",
                "edge",
                "--web-port=5000",
                "--web-enable-expression-evaluation"
            ]
        },
        {
            "name": "Flutter run CanvasKit",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
                "-d",
                "edge",
                "--web-renderer canvaskit",
                "--release",
                "--web-port=5000",
                "--web-enable-expression-evaluation"
            ]
        },
    ]
